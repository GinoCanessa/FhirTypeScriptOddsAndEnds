{
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": true,
    "target": "es2015",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    // "noEmit": true,
    "jsx": "react-jsx",

    // "composite": true,

    // "strictPropertyInitialization": false,

    "declaration": true,
    "declarationMap": true,
    // "noImplicitAny": true,
    // "noEmitOnError": true,
    // "removeComments": false,
    "sourceMap": true,
    // "target": "es2020",
    // "module": "es2020",
    // "lib": ["es2020"],
    "outDir": "dist",
    // "esModuleInterop": true,
    // "skipLibCheck": true,
    // "forceConsistentCasingInFileNames": true,
    //   // "preserveConstEnums": true,
    //   // "isolatedModules": true,
    //   // "strict": true,
    //   // "alwaysStrict": true,
    //   // "noImplicitThis": true,
    //   // "strictNullChecks": true,
    //   // "noFallthroughCasesInSwitch": true,
    //   // "noImplicitReturns": true,
    //   // "noUnusedLocals": false,
    //   // "noUnusedParameters": false,
    //   // "noEmit": true,
  },
  "include":[
    "src/*",
    "src/**/*"
  ],
  "exclude": [
    "Playground",
    "Playground/*",
    "Playground/**/*"
  ]
}